<?xml version="1.0" encoding="utf-8" ?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StoryCanvas.View.CustomControls.Cells.DateCell">
	<StackLayout Orientation="Horizontal" Padding="20,10,0,0" Spacing="10" HeightRequest="50">
		<Label x:Name="TextLabel"/>
		<DatePicker x:Name="DatePicker" VerticalOptions="CenterAndExpand" Format="D"/>
	</StackLayout>
</ViewCell>