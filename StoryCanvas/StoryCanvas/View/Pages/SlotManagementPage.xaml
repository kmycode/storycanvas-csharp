<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StoryCanvas.View.Pages.SlotManagementPage"
             xmlns:i18n="clr-namespace:StoryCanvas"
             Title="{i18n:Translate Slot}">
  <StackLayout>
    <Label Text="{i18n:Translate SaveFile}"/>
    <Button Text="{i18n:Translate SaveFile}" Command="{Binding SaveFileCommand}"/>
    <Label Text="{i18n:Translate LoadFile}"/>
    <Button Text="{i18n:Translate LoadFile}" Command="{Binding OpenFileCommand}"/>
  </StackLayout>
</ContentPage>